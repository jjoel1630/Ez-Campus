{
	"name": "Ez Campus",
	"description": "Extension to predict grades from the infinite campus portal",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": ["activeTab"],
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"https://fremontunifiedca.infinitecampus.org/campus/nav-wrapper/student/portal/student/classroom/*"
			],
			"js": ["embedPage.js"]
		}
	]
}
